<h1>Edição</h1>

<div class="container">
	<div class="row">
		<div class="span9">
			<div id="id_form_find">
				<form class="form_search"> 
					<input class="input-large search-query" id="term"  type="search" />
					<input class="btn btn btn-short btn-primary" id="search_bn_edit" value="Listar" />
				</form>
			</div>
		</div>
	</div>
</div>

<div class="container">
	<div id="id_form_result">

		<table class="table table-striped table-bordered table-condensed">
	  		<thead>
		          <tr>
		            <th>#</th>
		            <th>Nome</th>
		            <th>Ramal</th>
		            <th>Local</th>
		            <td>Editar</th>
		          </tr>
		        </thead>
		        <tbody id="id_body_edit_result">
		          
		        </tbody>
		</table>
	
	</div>
</div>


<div id="id_div_editmodal" class="modal fade hide" > 
	 <div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
		<h3>Editar Ramal</h3>
	</div>
	
	<div class="modal-body">
		<form class="form-horizontal" id="id_form_ramal_update"> 
			<div class="control-group">
				<input name="ramal[id]" id="id_input_id" type="hidden" placeholder="Nome" value="" />

				<label class="control-label" for="name">Nome</label>
				<div class="controls">
					<input name="ramal[name]" id="id_input_name" type="text" placeholder="Nome" value=""/>
				</div>
			</div>
			<div class="control-group">
				<label class="control-label" for="number">Numero</label>
				<div class="controls">
					<input id="id_input_number" name="ramal[number]" type="text" placeholder="Ramal" value=""/>
				</div>
			</div>
			<div class="control-group">
				<label class="control-label" for="local">Local</label>
				<div class="controls">
					<input id="id_input_local" name="ramal[local]" type="text" placeholder="Local" value=""/>
				</div>
			</div>
		</form>
	</div>

	 <div class="modal-footer">
		<a href="#" class="btn" data-dismiss="modal" aria-hidden="true">Cancelar</a>
		<a href="#" id="id_button_submit" class="btn btn-primary">Atualizar</a>
	</div>	
		
	
</div>